{
  "manifest_version": 3,
  "name": "说课",
  "version": "2.5.3",
  "description": "从文本中提取说客英语账号密码并自动填充",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "downloads"
  ],
  "host_permissions": [
    "https://www.talk915.com/*",
    "https://trmk.teachingrecord.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/talk_template.xlsx", "lib/xlsx.full.min.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.talk915.com/teacher/login*"],
      "js": ["content.js"]
    }
  ]
}